{
  "name": "bubbleboard",
  "version": "1.0.0",
  "description": "Visualize your data structure into a bubble map",
  "main": "index.js",
  "babel":{
    "plugins": ["transform-object-rest-spread"],
    "ignore": ["/node_modules/"]
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon ./server/server.js",
    "webpack": "webpack",
    "webpack:dev": "webpack-dev-server",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "dev": "concurrently \"npm run start\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BubbleBoard/bubbleboard"
  },
  "keywords": [
    "bubbleboard",
    "data",
    "structure",
    "bubble",
    "map",
    "visualize"
  ],
  "author": "Wunderpuss",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/slaglebagel17/bubbleboard/issues"
  },
  "homepage": "https://github.com/slaglebagel17/bubbleboard#readme",
  "dependencies": {
    "concurrently": "^3.6.1",
    "express": "^4.16.3",
    "konva": "^2.2.2",
    "matter-js": "^0.14.2",
    "mongoose": "^5.2.9",
    "nodemon": "^1.18.3",
    "react": "^16.4.2",
    "react-bootstrap": "^0.32.3",
    "react-dom": "^16.4.2",
    "react-force-graph": "^1.11.0",
    "react-p5-wrapper": "0.0.4",
    "react-redux": "^5.0.7",
